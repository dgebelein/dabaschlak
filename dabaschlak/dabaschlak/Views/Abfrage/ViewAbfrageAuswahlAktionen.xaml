<UserControl x:Class="dabaschlak.ViewAbfrageAuswahlAktionen"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:dabaschlak"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="800">

<Grid HorizontalAlignment="Stretch">
	
       <StackPanel Orientation="Vertical">
		
			<local:ViewAuswahlZeitraum />
			<local:ViewAuswahlVersuchsflaechen/>
         <local:ViewAuswahlKategorien/>
         <local:ViewAbfrageTexte />
			<local:ViewAbfrageSortierung/>
		</StackPanel>
		
            
</Grid>
</UserControl>
